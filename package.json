{
  "name": "commitlint-example",
  "scripts": {
    "commitlint": "commitlint --edit",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^3.0.0",
    "cz-git": "^1.7.1",
    "husky": "^8.0.3"
  }
}
